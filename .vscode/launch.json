{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Realty Alerts - Scheduler",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "tasks",
                "worker",
                "-B",
                "--loglevel=info"
            ],
            "cwd": "${workspaceFolder}/src",
            "justMyCode": false
        },
        {
            "name": "Python: Realty Alerts - UI",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/manage.py",
            "args": ["runserver"],
            "cwd": "${workspaceFolder}/src",
            "justMyCode": false,
            "django": true
        }
    ]
}
